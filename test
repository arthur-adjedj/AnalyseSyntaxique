var f0,f1,v,c,ga; //a

proc p
    do
    :: 1 -> //c

	f0 := 1; v := 0;
	do
	:: v == 1 || !f0  -> break
	od;

	c := c+1;
	c := c-1;

	f0 := 0
    od
end

proc p1
    do
    :: 1 ->   

	f1 := 1; v := 1;
	do
	:: v == 0 || !f1  -> break
	od;

	c := c+2;
	c := c-2;

	f1 := 0
    od
end

reach	c == 1	
reach	c == 2	
reach	c == 3	